
datafile = "soas.out"

set terminal postscript enhanced eps color dashed dl 5 lw 2;
set grid

set notitle
set multiplot layout 2,1
set lmargin at screen 0.1

set ylabel "process"
set key bottom right
set yrange [-1.5:1.5]
plot datafile using 1:2 title "desired" lc rgb "red" with lines, \
     datafile using 1:3 title "actual" lc rgb "blue" with lines

set notitle
set nokey
set ylabel "relay output"
set yrange [-0.6:0.6]
plot datafile using 1:4 title column(4) lc rgb "blue" with lines

