(*external print_hello: unit -> unit = "print_hello"*)
let hybrid main () = () where
  rec trigger = (period (1.))
  and automaton 
	| Forward -> do vel = 30 until trigger then StopF
	| StopF -> do vel = 0 until trigger then Backward
	| Backward -> do vel = -30 until trigger then StopB
	| StopB -> do vel = 0 until trigger then Forward
   end
  and () = present(trigger) ->
    ((print_int vel); print_newline ();
    move_robot(vel))
  else ()
