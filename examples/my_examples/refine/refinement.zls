(* Tuning Fork *)

let pi = 3.14159
let w = pi*.2.0
let y0:float{y0 >= 0.0} = 1.0
let y'0 = 0.0

let hybrid main () = () where
  rec der y  =  y' init y0
  and der y' = -.w*.w*.y init y'0
  and f = assume(0.01) 
  and trigger = period(0.01) and
  present trigger -> local cpt in
    do  cpt = 0 fby cpt + 1
    and  () = print_endline (string_of_float last y) done
